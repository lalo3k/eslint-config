{
  "name": "@lalo3k/eslint-config",
  "version": "1.0.1",
  "description": "These are settings for ESLint and Prettier that I use in my projects.",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "test",
    "publish": "npm publish && git push",
    "publish:patch": "npm version patch && npm run publish",
    "publish:minor": "npm version minor -git-tag-version && npm run publish",
    "publish:major": "npm version major -git-tag-version && npm run publish",
    "publish:prerelease": "npm version prerelease -git-tag-version && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lalo3k/eslint-config.git"
  },
  "keywords": [
    "eslintconfig",
    "eslint"
  ],
  "author": "lalo3k",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lalo3k/eslint-config/issues"
  },
  "homepage": "https://github.com/lalo3k/eslint-config#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=5.56.0",
    "eslint": ">=8.36.0",
    "eslint-config-prettier": ">=8.8.0",
    "eslint-plugin-import": ">=2.27.5",
    "eslint-plugin-jsx": ">=0.1.0",
    "eslint-plugin-prettier": ">=4.2.1",
    "eslint-plugin-simple-import-sort": ">=10.0.0",
    "prettier": ">=2.8.7"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.56.0"
  }
}
